<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Direccion</servlet-name>
        <servlet-class>user.Direccion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DireccionActions</servlet-name>
        <servlet-class>user.DireccionActions</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutUser</servlet-name>
        <servlet-class>user.LogoutUser</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>user.User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserActions</servlet-name>
        <servlet-class>user.UserActions</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserRegister</servlet-name>
        <servlet-class>user.UserRegister</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Direccion</servlet-name>
        <url-pattern>/Direccion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DireccionActions</servlet-name>
        <url-pattern>/DireccionActions</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutUser</servlet-name>
        <url-pattern>/LogoutUser</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/User</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserActions</servlet-name>
        <url-pattern>/UserActions</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserRegister</servlet-name>
        <url-pattern>/UserRegister</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
